<h3><%=@album.title %> </h3>

<table>
<th>year</th>
<th>recording</th>
<th>band</th>
<tr>
  <td> <%= @album.year %> </td>
  <% if @album.studio_album == true  %>
  <td> <%= "Studio" %>  </td>
  <% else %>
    <td> <%= "Live" %>  </td>
  <% end %>
  <td> <%= @album.band.name  %></td>
</tr>

</table>
